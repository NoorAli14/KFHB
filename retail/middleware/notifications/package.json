{
  "name": "nestjs-knex-graphql-starter-kit",
  "version": "0.0.1",
  "description": "",
  "author": "Faizan Ahmad <faizan@aiondigital.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "db:migrate": "knex --knexfile database.ts  migrate:latest",
    "db:rollback": "knex --knexfile database.ts migrate:rollback",
    "migrate:make": "knex migrate:make -x ts --knexfile database.ts",
    "seed:make": "knex seed:make -x ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "update:packages": "npm ncu -u",
    "generate:docs": "npx compodoc -p tsconfig.json -s",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/federation": "^0.17.0",
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.5.1",
    "@nestjs/microservices": "^7.3.2",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.5.11",
    "@nestjs/terminus": "^7.0.1",
    "apollo-server-express": "^2.15.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "fcm-node": "^1.5.2",
    "firebase-admin": "^9.1.1",
    "graphql": "^14.7.0",
    "graphql-tools": "^6.0.12",
    "knex": "^0.21.1",
    "moment": "^2.27.0",
    "mssql": "^6.2.0",
    "nestjs-knex": "^1.0.1",
    "nodemailer": "^6.4.11",
    "pg": "^8.2.1",
    "pug": "^3.0.0",
    "randomatic": "^3.1.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "tedious": "^8.3.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.2.3",
    "@types/lodash": "^4.14.157",
    "@types/node": "12.12.43",
    "@types/readable-stream": "2.3.5",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.2.5",
    "jest": "26.0.1",
    "npm-check-updates": "^7.0.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {}
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
