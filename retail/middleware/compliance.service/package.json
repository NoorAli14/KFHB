{
  "name": "rubix-complaince-service",
  "version": "0.0.1",
  "description": "",
  "author": "Faizan Ahmad <faizan@aiondigital.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "db:migrate": "knex --knexfile database.ts  migrate:latest",
    "db:rollback": "knex --knexfile database.ts migrate:rollback --all",
    "migrate:make": "knex migrate:make -x ts --knexfile database.ts",
    "seed:make": "knex seed:make -x ts --knexfile database.ts",
    "db:seed": "knex seed:run --knexfile database.ts",
    "generate:schema": "node --require ts-node/register generate-sql-schema.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "update:packages": "npm ncu -u",
    "generate:docs": "npx compodoc -p tsconfig.json -s",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@apollo/federation": "^0.17.0",
    "@nestjs/common": "^7.4.4",
    "@nestjs/core": "^7.4.4",
    "@nestjs/graphql": "^7.7.0",
    "@nestjs/microservices": "^7.4.4",
    "@nestjs/platform-express": "^7.4.4",
    "@nestjs/swagger": "^4.6.1",
    "@nestjs/terminus": "^7.0.1",
    "@types/cls-hooked": "^4.3.1",
    "apollo-server-express": "^2.18.1",
    "class-transformer": "^0.2.3",
    "class-transformer-validator": "^0.9.1",
    "class-validator": "^0.12.2",
    "cls-hooked": "^4.2.2",
    "dataloader": "^2.0.0",
    "dotenv": "^8.2.0",
    "graphql": "^14.7.0",
    "graphql-tools": "^6.2.3",
    "knex": "^0.21.5",
    "lodash": "^4.17.20",
    "mssql": "^6.2.3",
    "nestjs-dataloader": "^7.0.1",
    "nestjs-knex": "^1.0.1",
    "pg": "^8.3.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.4",
    "tedious": "^8.3.0"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.2",
    "@nestjs/testing": "^7.4.4",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.8",
    "@types/jest": "25.2.3",
    "@types/lodash": "^4.14.161",
    "@types/node": "12.12.43",
    "@types/readable-stream": "2.3.5",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.0.2",
    "@typescript-eslint/parser": "3.0.2",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "7.1.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.20.1",
    "husky": "^4.3.0",
    "jest": "26.0.1",
    "npm-check-updates": "^7.1.1",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "26.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  },
  "husky": {
    "hooks": {}
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}