{
    "name": "rubix-schema-generation",
    "version": "0.0.2",
    "description": "This module is used to generate a SQL schema from knex migrations",
    "author": "Faizan Ahmad <faizan@aiondigital.com>",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "gs:all": "npm run gs:compliance && npm run gs:entitlements && npm run gs:identity && npm run gs:notifications && npm run gs:reference && npm run gs:videocall",
        "gs:compliance": "cross-env RBX_SERVICE_NAME=compliance.service ts-node --project ../../../retail/middleware/compliance.service/tsConfig.json --require ./tsconfig-bootstrap.js index.ts",
        "gs:entitlements": "cross-env RBX_SERVICE_NAME=entitlement.service ts-node --project ../../../retail/middleware/entitlement.service/tsConfig.json --require ./tsconfig-bootstrap.js index.ts",
        "gs:identity": "cross-env RBX_SERVICE_NAME=identity.service ts-node --project ../../../retail/middleware/identity.service/tsConfig.json --require ./tsconfig-bootstrap.js index.ts",
        "gs:notifications": "cross-env RBX_SERVICE_NAME=notifications ts-node --project ../../../retail/middleware/notifications/tsConfig.json --require ./tsconfig-bootstrap.js index.ts",
        "gs:reference": "cross-env RBX_SERVICE_NAME=reference.service ts-node --project ../../../retail/middleware/reference.service/tsConfig.json --require ./tsconfig-bootstrap.js index.ts",
        "gs:videocall": "cross-env RBX_SERVICE_NAME=videocall.service ts-node --project ../../../retail/middleware/videocall.service/tsConfig.json --require ./tsconfig-bootstrap.js index.ts",
        "generate:seed:all": "npm run gsd:compliance && npm run gsd:entitlements && npm run gsd:identity && npm run gsd:reference",
        "gsd:compliance": "cross-env RBX_SERVICE_NAME=compliance.service ts-node --project ../../../retail/middleware/compliance.service/tsConfig.json --require ./tsconfig-bootstrap.js generate-seed.ts",
        "gsd:entitlements": "cross-env RBX_SERVICE_NAME=entitlement.service ts-node --project ../../../retail/middleware/entitlement.service/tsConfig.json --require ./tsconfig-bootstrap.js generate-seed.ts",
        "gsd:identity": "cross-env RBX_SERVICE_NAME=identity.service ts-node --project ../../../retail/middleware/identity.service/tsConfig.json --require ./tsconfig-bootstrap.js generate-seed.ts",
        "gsd:reference": "cross-env RBX_SERVICE_NAME=reference.service ts-node --project ../../../retail/middleware/reference.service/tsConfig.json --require ./tsconfig-bootstrap.js generate-seed.ts"
    },
    "dependencies": {
        "@types/chalk": "^0.4.31",
        "chalk": "^1.1.3",
        "knex": "^0.21.5",
        "module-alias": "^2.2.2",
        "moment": "^2.29.1",
        "mssql": "^6.2.2",
        "pg": "^8.3.3",
        "tedious": "^8.3.1",
        "uuid": "^8.3.0"
    },
    "devDependencies": {
        "@types/dotenv": "^8.2.0",
        "@types/node": "^12.12.43",
        "cross-env": "^7.0.2",
        "ts-loader": "^6.2.1",
        "ts-node": "^8.6.2",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.9.7"
    }
}
